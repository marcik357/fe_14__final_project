{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-trailing-spaces": ["error", { "skipBlankLines": true } ],
        "linebreak-style": ["error", "windows"],
        "no-return-assign": 0,
        "eol-last": 0,
        "react/react-in-jsx-scope": "off",
        "import/prefer-default-export": "off"
    }
}